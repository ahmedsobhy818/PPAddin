					<gallery idMso="OutlineDashesGallery" showLabel="false" label="&#x200B;" />
					<gallery idMso="OutlineWeightGallery" showLabel="false" label="&#x200B;" />
					<button idMso="EyedropperOutline" showLabel="false" />
